<h1>Wish Saver</h1>

<h3>Save Wishes</h3>

<ul>
    {{#each burgers}}
    <li>
        <p>ID: {{this.id}}</p>
        <p>Burger: {{this.name}}</p>
    </li>
    {{/each}}
</ul>

<div>
    <form action="/burgers" method="POST">
        <textarea type="text" name="burger"></textarea>

        <button type="submit">Submit</button>
    </form>
</div>